{
	"name": "kittyrescue",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"start": "node node_modules/react-native/local-cli/cli.js start",
		"test": "jest",
		"test:watch": "jest --watch",
		"compile": "npm run copy-img && tsc -w",
		"copy-img": "xcopy src\\img dist\\img /i/y"
	},
	"dependencies": {
		"guid": "0.0.12",
		"native-base": "^2.3.3",
		"ramda": "^0.25.0",
		"react": "16.0.0",
		"react-native": "0.50.4",
		"react-native-elements": "^0.19.1",
		"react-native-sideswipe": "^1.4.2",
		"react-native-sound": "^0.10.9",
		"react-native-vector-icons": "^4.6.0",
		"react-navigation": "^1.0.0-beta.22",
		"react-navigation-slide-from-right-transition": "^1.0.4",
		"react-redux": "^5.0.6",
		"realm": "^2.1.1",
		"redux": "^3.7.2"
	},
	"devDependencies": {
		"@types/jest": "^22.2.2",
		"@types/ramda": "^0.25.18",
		"@types/react": "^16.0.38",
		"@types/react-native": "^0.52.12",
		"babel-jest": "21.2.0",
		"babel-preset-react-native": "4.0.0",
		"jest": "21.2.1",
		"lodash.debounce": "^4.0.8",
		"react-test-renderer": "16.0.0",
		"ts-jest": "^22.4.2",
		"typescript": "^2.8.1"
	},
	"rnpm": {
		"assets": [
			"./assets/fonts/"
		]
	},
	"jest": {
		"preset": "react-native",
		"transform": {
			"^.+\\.tsx?$": "ts-jest"
		},
		"testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
		"moduleFileExtensions": [
			"ts",
			"tsx",
			"js",
			"jsx",
			"json",
			"node"
		],
		"transformIgnorePatterns": [
			"node_modules/(?!react-native|native-base-shoutem-theme|@shoutem/animation|@shoutem/ui|tcomb-form-native)"
		]
	}
}
